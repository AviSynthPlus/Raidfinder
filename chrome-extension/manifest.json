{
  "name": "Raid ID copier Companion",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Join your GBF raids",
  "background" : {
    "scripts" : [ "background.js" ],
    "persistent": false
  },
  "browser_action":{
    "default_icon":"icon-128.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "clipboardRead",
    "activeTab",
    "http://game.granbluefantasy.jp/*"
  ]
}
